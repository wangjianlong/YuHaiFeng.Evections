@{ 
    Layout = null;
    Dictionary<string, List<YuHaiFeng.Evections.Models.Evection_User_View>> dict = ViewBag.Dict;
    int serialNumber = 1;
}

<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>序号</th>
            <th>时间</th>
            <th>出差人员</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var entry in dict)
        {
            <tr>
                <td rowspan="@(entry.Value.Count+1)">@(serialNumber++)</td>
                <td rowspan="@(entry.Value.Count+1)">@(entry.Key)</td>
            </tr>
            foreach(var item in entry.Value)
            {
                <tr>
                    <td>@(item.Name)</td>
                </tr>
            }
        }
    </tbody>
</table>

