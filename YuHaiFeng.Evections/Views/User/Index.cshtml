@{
    ViewBag.Title = "用户列表";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<YuHaiFeng.Evections.Models.User> users = ViewBag.List;
}

<h3 class="text-center">用户列表</h3>
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>序号</th>
            <th>用户名称</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in users)
        {
            <tr>
                <td>@(user.ID)</td>
                <td>@(user.Name)</td>
            </tr>
        }
    </tbody>
</table>
